<!DOCTYPE html>
<html data-wf-page="598a2125fda8db00018020a0" data-wf-site="598874739abf440001d32417">
<head>
  <meta charset="utf-8">
  <title>mbnb</title>
  <meta content="Layout" property="og:title">
  <meta content="width=device-width, initial-scale=1" name="viewport">
  <link href="<%= url("/css/normalize.css")%>" rel="stylesheet" type="text/css">
  <link href="<%= url("/css/webflow.css")%>" rel="stylesheet" type="text/css">
  <link href="<%= url("/css/makersbnb.webflow.css")%>" rel="stylesheet" type="text/css">
  <script src="https://ajax.googleapis.com/ajax/libs/webfont/1.4.7/webfont.js" type="text/javascript"></script>
  <script type="text/javascript">WebFont.load({
  google: {
    families: ["Droid Sans:400,700"]
  }
});</script>
  <script type="text/javascript">!function(o,c){var n=c.documentElement,t=" w-mod-";n.className+=t+"js",("ontouchstart"in o||o.DocumentTouch&&c instanceof DocumentTouch)&&(n.className+=t+"touch")}(window,document);
</script>


<!-- PICKER SCRIPTS STARTS -->
<link rel="stylesheet" href="<%= url("/css/jquery-ui.css")%>">
<script src="https://ajax.googleapis.com/ajax/libs/jquery/2.2.0/jquery.min.js" type="text/javascript"></script>
<script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
<script>
$( document ).ready( function() {
  $( "#startDate" ).datepicker();
  $( "#endDate" ).datepicker();
} );
</script>
<!-- PICKER SCRIPT ENDS -->


  <link href="https://daks2k3a4ib2z.cloudfront.net/img/favicon.ico" rel="shortcut icon" type="image/x-icon">
  <link href="https://daks2k3a4ib2z.cloudfront.net/img/webclip.png" rel="apple-touch-icon">
</head>
<body class="body">


  <!-- START INTEGRATION -->
  <% if !current_user %>
  <div class="navbar w-nav" data-animation="default" data-collapse="small" data-duration="400">
    <div class="w-container">
      <a class="w-nav-brand" href="/">
        <div class="logo">mbnb</div>
      </a>
      <nav class="navmenu w-nav-menu" role="navigation"><a class="navlink w-nav-link" href="https://github.com/Nandhini31/Makersbnb" target="blank">About</a><a class="navlink w-nav-link" href="/log_in">Login</a></nav>
      <div class="menubutton w-nav-button" data-ix="navbar">
        <div class="hambtop"></div>
        <div class="hambmid"></div>
        <div class="hambbottom"></div>
      </div>
    </div>
  </div>
  <% end %>
  <!-- END INTEGRATION -->


  <!-- START INTEGRATION -->
  <% if current_user %>
  <div class="navbar w-nav" data-animation="default" data-collapse="small" data-duration="400">
    <div class="w-container">
      <a class="w-nav-brand" href="/">
        <div class="logo">mbnb</div>
      </a>
      <nav class="navmenu w-clearfix w-nav-menu" role="navigation"><a class="navlink w-nav-link" href="/listings">Listings</a><a class="navlink w-nav-link" href="/dashboard">My Dashboard</a>
        <div class="formmenu w-form">
          <form action="/logout" class="w-clearfix" data-name="logout" id="wf-form-logout" method="post" name="wf-form-logout">
            <div class="w-embed"><input type="hidden" name="_method" value="delete"></div><input class="button menu w-button" type="submit" value="Log out"></form>
        </div>
      </nav>
      <div class="menubutton w-nav-button" data-ix="navbar">
        <div class="hambtop"></div>
        <div class="hambmid"></div>
        <div class="hambbottom"></div>
      </div>
    </div>
  </div>
  <% end %>
  <!-- END INTEGRATION -->


  <!-- START INTEGRATION -->
  <% if flash[:errors] %>
  <div class="errors section">
    <div class="containergeneral w-container">
      <ol class="ulflash">
        <% flash[:errors].each do |error| %>
        <li class="liflash">
          <div class="bodycopy white"><%= error %></div>
        </li>
        <% end %>
      </ol>
    </div>
  </div>
  <% end %>
  <!-- END INTEGRATION -->


  <!-- START INTEGRATION -->
  <% if flash[:notice] %>
  <div class="notices section">
    <div class="containergeneral w-container">
      <ol class="dark ulflash">
        <li class="liflash">
          <div class="bodycopy"><%= flash[:notice]%></div>
        </li>
      </ol>
    </div>
  </div>
  <% end %>
  <!-- END INTEGRATION -->


  <!-- START INTEGRATION -->
  <%= yield %>
  <!-- END INTEGRATION -->


  <div class="footer">
    <div class="w-container">
      <div class="smallcopy">To the maximum extent allowed by law mbnb, its representatives, employees and directors are not liable for any direct or indirect damages that result from use or inability to use of its services. By using mbnb you are personally responsible for compliance with all the international and local laws.</div>
    </div>
  </div>
  <!-- <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.2.0/jquery.min.js" type="text/javascript"></script> -->
  <script src="<%= url("/js/webflow.js")%>" type="text/javascript"></script>
</body>
</html>
